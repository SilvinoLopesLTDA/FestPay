{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "build": "npm run build",
  "cleanUrls": true,
  "devCommand": "npm run dev",
  "framework": "vite",
  "installCommand": "npm install --force",
  "public": true
}
